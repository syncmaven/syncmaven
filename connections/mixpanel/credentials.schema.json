{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "projectToken": {
      "type": "string"
    },
    "residency": {
      "type": ["string", "null"],
      "enum": ["EU", "US"]
    },
    "initialSyncDays": {
      "type": ["integer", "null"],
      "default": 30,
      "minimum": 1
    },
    "lookbackWindow": {
      "type": ["integer", "null"],
      "default": 2,
      "minimum": 1
    }
  },
  "required": ["projectToken"]
}
