{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "date": {
      "type": "string",
      "format": "date"
    },
    "source": {
      "type": "string"
    },
    "campaign_id": {
      "type": ["string", "integer"]
    },
    "group_id": {
      "type": ["string", "integer", "null"]
    },
    "ad_id": {
      "type": ["string", "integer", "null"]
    },
    "campaign_name": {
      "type": ["string", "null"]
    },
    "cost": {
      "type": ["number", "null"]
    },
    "clicks": {
      "type": ["number", "null"]
    },
    "impressions": {
      "type": ["number", "null"]
    },
    "conversions": {
      "type": ["number", "null"]
    },
    "utm_source": {
      "type": ["string", "null"]
    },
    "utm_medium": {
      "type": ["string", "null"]
    },
    "utm_campaign": {
      "type": ["string", "null"]
    },
    "utm_content": {
      "type": ["string", "null"]
    },
    "utm_term": {
      "type": ["string", "null"]
    }
  },
  "required": ["date", "source", "campaign_id"]
}
