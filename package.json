    "lodash": "^4.17.21",
    "nunjucks": "^3.2.4",
    "oauth-1.0a": "^2.2.6",
    "pg": "^8.11.5",
    "pg-cursor": "^2.10.5",
    "pg-query-stream": "^4.5.5",
    "request": "^2.88.2",
    "resend": "^3.2.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "twitter-ads": "^0.2.1",
    "zod": "^3.23.4"
  }
